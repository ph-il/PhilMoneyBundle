{
    "name": "ph-il/money-bundle",
    "type": "symfony-bundle",
    "description": "This is a Symfony bundle that integrates moneyphp/money library (Fowler pattern): https://github.com/moneyphp/money.",
    "keywords": ["money", "currency", "fowler", "conversion"],
    "homepage": "https://github.com/ph-il/PhilMoneyBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Gamache",
            "homepage": "https://twitter.com/philoupedia"
        },
        {
            "name": "Philippe Le Van",
            "homepage": "https://twitter.com/plv"
        },
        {
            "name": "Sebastien Lefebvre"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl" : "*",
        "symfony/framework-bundle": "^4.0",
        "moneyphp/money": "^3.0",
        "symfony/form": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dom-crawler": "^4.0"
    },
    "require-dev": {
        "symfony/class-loader": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/browser-kit": "^4.0",
        "beberlei/DoctrineExtensions": "0.3.*",
        "ext-sqlite3": "*",
        "doctrine/doctrine-bundle": "~1.8",
        "doctrine/orm": "~2.6",
        "phpunit/phpunit": "~4|~5",
        "phpunit/dbunit": "~1.4",
        "symfony/phpunit-bridge": "^4.0",
        "squizlabs/php_codesniffer": "^2.9",
        "m6web/Symfony2-coding-standard": "^3.3"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "~1.8",
        "doctrine/orm": "~2.6"
    },
    "autoload": {
        "psr-0": { "Phil\\MoneyBundle": "" }
    },
    "autoload-dev": {
        "classmap": [ "Tests/app/AppKernel.php" ]
    },
    "target-dir": "Phil/MoneyBundle"
}
